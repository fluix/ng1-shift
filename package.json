{
  "name": "ng1-shift",
  "version": "0.0.6",
  "description": "Angular 1.5+ decorators for writing Angular2 like.",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --watchAll",
    "postinstall": "npm run build && npm run vendor",
    "vendor": "rollup --config rollup.config.vendor.js ",
    "build": "rollup --config rollup.config.dist.js",
    "start": "rollup --config rollup.config.dev.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/readdle/ng1-shift.git"
  },
  "keywords": [
    "angular",
    "angularJS",
    "ng",
    "ng1",
    "components",
    "migrate"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/readdle/ng1-shift/issues"
  },
  "homepage": "https://github.com/readdle/ng1-shift#readme",
  "jest": {
    "transform": {
      ".ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(\\.(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "dependencies": {
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.10",
    "rollup": "0.41.6",
    "rollup-plugin-alias": "1.3.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript": "0.8.1",
    "typescript": "2.3.4"
  },
  "devDependencies": {
    "@types/angular": "1.6.17",
    "@types/jest": "19.2.4",
    "angular": "1.6.4",
    "jest": "20.0.4",
    "rollup-watch": "3.2.2",
    "ts-jest": "20.0.5",
    "tslib": "1.7.1"
  }
}
